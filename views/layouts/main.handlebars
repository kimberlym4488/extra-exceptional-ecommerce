<html lang='en'>

  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Extra Exceptional e-Commerce</title>
    <link rel='stylesheet' href='./css/style.css' />
    <link rel='stylesheet' href='./css/branding.css' />
    <link rel='stylesheet' href='./css/bs-overrides.css' />
    {{! Bootstrap Styles/Icons }}
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css'
    />
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3'
      crossorigin='anonymous'
    />

    {{! jquery min script }}
    <script
      src='https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js'
    ></script>

    {{! Bootstrap Script }}

    <script
      src='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js'
      integrity='sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p'
      crossorigin='anonymous'
    ></script>

  </head>

  <body>
    <nav class='navbar navbar-light bg-light'>
      <div class='container-fluid'>
        <span class='navbar-brand mb-0 h1'>Exceptional e-Commerce</span>
      </div>

      <div class='container-fluid'>
      </div>
    </nav>

    <div class='card text-center'>

      <div class='card-body'>
        <h5 class='card-title'>What do you need to do today?</h5>
        <p class='card-text'>Click an option below to get started. </p>

        <div class='btn-group'>
          <a
            href='api/categories'
            class='btn btn-primary active'
            aria-current='page'
          >Warehouses
          </a>
          <a
            href='api/products'
            class='btn btn-primary active'
            aria-current='page'
          >Products
          </a>
          <a
            href='api/tags'
            class='btn btn-primary active'
            aria-current='page'
          >Tags
          </a>
        
        </div>

      </div>
      {{{body}}}
    </div>
    <footer class='bg-secondary px-3'>
      <div class='text-center'>
        <a href='https://github.com/kimberlym4488/extra-exceptional-ecommerce'>
          <img
            src='./assets/GitHub-Emblem.png'
            style='width:80px;height:40px'
          /></a>
      </div>
    </footer>
    {{! Bootstrap and Jquery placed here to apply to each component/view rendered within }}
    <script
      src='https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js'
      integrity='sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj'
      crossorigin='anonymous'
    ></script>
    <script
      src='https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js'
      integrity='sha384-fQybjgWLrvvRgtW6bFlB7jaZrFsaBXjsOMm/tB9LTS58ONXgqbR9W8oWht/amnpF'
      crossorigin='anonymous'
    ></script>
  </body>

</html>